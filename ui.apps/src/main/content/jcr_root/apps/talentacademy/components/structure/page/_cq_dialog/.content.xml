<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="cq-siteadmin-admin-properties-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <advanced
                        jcr:primaryType="nt:unstructured"
                        path="/mnt/override/apps/talentacademy/components/structure/page/tabs/advanced"/>
                    <seo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SEO"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <robotsTags
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        emptyText="Select"
                                        fieldDescription="Select the robots tags to control search engine crawler behavior."
                                        fieldLabel="Robots Tag"
                                        multiple="{Boolean}true"
                                        name="./robotsTags">
                                        <items jcr:primaryType="nt:unstructured">
                                            <index
                                                jcr:primaryType="nt:unstructured"
                                                text="index"
                                                value="index"/>
                                            <noindex
                                                jcr:primaryType="nt:unstructured"
                                                text="noindex"
                                                value="noindex"/>
                                            <follow
                                                jcr:primaryType="nt:unstructured"
                                                text="follow"
                                                value="follow"/>
                                            <nofollow
                                                jcr:primaryType="nt:unstructured"
                                                text="nofollow"
                                                value="nofollow"/>
                                        </items>
                                    </robotsTags>
                                    <twitterCard
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        emptyText="Select"
                                        fieldDescription="Select the twitter card that determines the type of card to use."
                                        fieldLabel="Twitter Card"
                                        name="./twitterCard">
                                        <items jcr:primaryType="nt:unstructured">
                                            <summary
                                                jcr:primaryType="nt:unstructured"
                                                text="summary"
                                                value="summary"/>
                                            <summaryLargeImage
                                                jcr:primaryType="nt:unstructured"
                                                text="summary large image"
                                                value="summary large image"/>
                                            <app
                                                jcr:primaryType="nt:unstructured"
                                                text="app"
                                                value="app"/>
                                            <player
                                                jcr:primaryType="nt:unstructured"
                                                text="player"
                                                value="player"/>
                                        </items>
                                    </twitterCard>
                                    <twitterDescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/textarea"
                                        fieldLabel="Twitter Description"
                                        name="./twitterDescription"/>
                                    <twitterImage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/pathbrowser"
                                        fieldLabel="Twitter Image"
                                        name="./twitterImage"/>
                                    <twitterImageAltText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/textfield"
                                        fieldLabel="Twitter Image Alt Text"
                                        name="./twitterImageText"/>
                                    <twitterSite
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/textfield"
                                        fieldLabel="Twitter Site Name"
                                        name="./twitterSite"/>
                                    <twitterTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/textfield"
                                        fieldLabel="Twitter Title"
                                        name="./twitterTitle"/>
                                    <ogType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        emptyText="Select"
                                        fieldDescription="Select the ogType that determines the type of object shared"
                                        fieldLabel="og Type"
                                        name="./ogType">
                                        <items jcr:primaryType="nt:unstructured">
                                            <product
                                                jcr:primaryType="nt:unstructured"
                                                text="product"
                                                value="product"/>
                                            <website
                                                jcr:primaryType="nt:unstructured"
                                                text="website"
                                                value="website"/>
                                        </items>
                                    </ogType>
                                    <ogTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/textfield"
                                        fieldLabel="og Title"
                                        name="./ogTitle"/>
                                    <ogDescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/textarea"
                                        fieldLabel="og Description"
                                        name="./ogDescription"/>
                                    <ogSite
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/textfield"
                                        fieldLabel="og Site Name"
                                        name="./ogSite"/>
                                    <ogImage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/pathbrowser"
                                        fieldLabel="og Image"
                                        name="./ogImage"/>
                                    <ogImageAltText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/textfield"
                                        fieldLabel="og Image Alt Text"
                                        name="./ogImageText"/>
                                </items>
                            </column>
                        </items>
                    </seo>
                    <customConfiguration
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Custom Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">                                    
                                    <headerButtonLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/textfield"
                                        fieldLabel="Header Button Label"
                                        name="./headerButtonLabel"/>
                                    <headerButtonUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation//form/pathbrowser"
                                        fieldLabel="Header Button URL"
                                        rootPath="/content/talentacademy"
                                        name="./headerButtonUrl"/>
                                    <faviconsRoot
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Please select the fav icons image path."
                                        fieldLabel="Favicon Image"
                                        name="./faviconPath"
                                        rootPath="/content/dam/talentacademy">
                                    </faviconsRoot>
                                    <direction
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Language Direction"
                                            fieldDescription="Select the language direction"
                                            name="./languageDirection">
                                        <items jcr:primaryType="nt:unstructured">
                                            <ltr
                                                jcr:primaryType="nt:unstructured"
                                                text="Left to Right"
                                                value="ltr"/>
                                            <rtl
                                                jcr:primaryType="nt:unstructured"
                                                text="Right to Left"
                                                value="rtl"/>
                                        </items>
                                    </direction>
                                    <hideInSiteMap
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./hideInSiteMap"
                                        text="Hide in Site Map"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </column>
                        </items>
                    </customConfiguration>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
